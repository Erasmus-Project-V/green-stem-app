# File name: comiccreator.kv
<ComicCreator@Screen>:
    id: "ccr"
    name: 'comicscreen'
    FloatLayout:
        MDLabel:
            id: textlabel
            text: "ok...."
            text_color: (1,1,1,1)
            pos_hint: {'center_x':0.1,'center_y':.5}
        MDFillRoundFlatButton:
            text: "go back"
            pos_hint: {'center_x': .75, 'y': .05}
            size_hint: 0.2,0.2
            on_press:
                root.clicked()
                root.manager.current = 'cps'
        TextInput:
            pos_hint: {'center_x': .75, 'y': .5}
            size_hint: 0.2,0.2
